{
  "name": "@oku-ui/motion",
  "type": "module",
  "version": "0.2.1",
  "packageManager": "pnpm@8.13.0",
  "description": "A tiny, performant animation library for VueJS",
  "author": "Mehmet productdevbook <hi@productdevbook.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/productdevbook",
  "homepage": "https://github.com/oku/oku-ui/motion",
  "repository": {
    "type": "git",
    "url": "https://github.com/oku/oku-ui/motion.git"
  },
  "bugs": "https://github.com/oku/oku-ui/motion/issues",
  "keywords": [
    "vue",
    "motion"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    }
  },
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "playground": "pnpm --filter playground-vue dev",
    "clean": "rimraf ./dist && rimraf ./node_modules",
    "release": "pnpm build && pnpm bumpp --commit='version(vue): release %s' --no-tag && pnpm publish"
  },
  "peerDependencies": {
    "vue": ">=3.3.0"
  },
  "dependencies": {
    "@motionone/dom": "^10.16.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "2.6.1",
    "@types/node": "^20.10.6",
    "@vitejs/plugin-vue": "^5.0.2",
    "@vitest/coverage-v8": "^1.1.1",
    "@vue-macros/volar": "^0.18.8",
    "@vue/test-utils": "^2.4.3",
    "bumpp": "^9.2.1",
    "eslint": "^8.56.0",
    "jsdom": "^23.0.1",
    "lint-staged": "^15.2.0",
    "simple-git-hooks": "^2.9.0",
    "typescript": "^5.3.3",
    "unplugin-vue-macros": "^2.7.7",
    "vite": "^5.0.10",
    "vite-plugin-dts": "^3.7.0",
    "vitest": "^1.1.1",
    "vue": "^3.4.3"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "pnpm eslint . --fix"
  },
  "publishConfig": {
    "access": "public"
  }
}
